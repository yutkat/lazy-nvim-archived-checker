{
  "problemMatcher": [
    {
      "owner": "lazy-nvim-archived-checker",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\[ARCHIVED\\]\\s+(.+):\\s*(.+)$",
          "file": 1,
          "message": 2
        }
      ]
    },
    {
      "owner": "lazy-nvim-moved-checker",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\[MOVED\\]\\s+(.+)\\s*->\\s*(.+):\\s*(.+)$",
          "file": 1,
          "message": 3
        }
      ]
    },
    {
      "owner": "lazy-nvim-deleted-checker",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^\\[DELETED\\]\\s+(.+):\\s*(.+)$",
          "file": 1,
          "message": 2
        }
      ]
    }
  ]
}
